{
  "name": "web",
  "version": "1.0.0",
  "description": "a custom web framework in typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "execute": "nodemon build/index.js",
    "dev:seq": "tsc && nodemon build/index.js",
    "dev:db": "json-server -w -p 3001 db.json",
    "dev": "concurrently npm:dev:*",
    "web:db": "json-server -w -p 3002 db.json",
    "web:parcel": "parcel -p 7777 index.html",
    "web": "concurrently npm:web:*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frzhen/typescript-web-framework.git"
  },
  "keywords": [
    "web",
    "framework"
  ],
  "author": "Fred Zhen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frzhen/typescript-web-framework/issues"
  },
  "homepage": "https://github.com/frzhen/typescript-web-framework#readme",
  "devDependencies": {
    "@types/node": "^18.11.12",
    "axios": "^1.2.1",
    "concurrently": "^7.6.0",
    "json-server": "^0.17.1",
    "nodemon": "^2.0.20"
  }
}
